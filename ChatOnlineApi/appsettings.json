{
  "ConnectionStrings": {
    "ChatOnlineDatabase": "Server=DESKTOP-RLQCKGK;Database=ChatOnline;Trusted_Connection=true",
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Temp\\Logs\\log.txt",
          "outputTemplate": "{Timestamp} {Message}{NewLine:1}{Exception:1}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl" :  "http://localhost:8081"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
